{
  "name": "solid-sfc",
  "version": "0.0.11",
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/production/index.js",
  "module": "dist/esm/production/index.js",
  "exports": {
    ".": {
      "development": {
        "require": "./dist/cjs/development/index.js",
        "import": "./dist/esm/development/index.js"
      },
      "require": "./dist/cjs/production/index.js",
      "import": "./dist/esm/production/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dev": {
      "production": {
        "require": "./dist/cjs/production/index.js",
        "import": "./dist/esm/production/index.js"
      },
      "require": "./dist/cjs/development/index.js",
      "import": "./dist/esm/development/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./esm": {
      "development": "./dist/esm/development/index.js",
      "production": "./dist/esm/production/index.js",
      "default": "./dist/esm/production/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./cjs": {
      "development": "./dist/cjs/development/index.js",
      "production": "./dist/cjs/production/index.js",
      "default": "./dist/cjs/production/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "env.d.ts"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "keywords": [
    "pridepack"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "eslint": "^7.32.0",
    "eslint-config-lxsmnsyc": "^0.3.0",
    "pridepack": "1.0.4",
    "ts-jest": "^27.1.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/generator": "^7.16.5",
    "@babel/parser": "7.16.6",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/types": "^7.16.0",
    "babel-plugin-solid-labels": "^0.9.6",
    "babel-preset-solid": "^1.2.6",
    "source-map": "^0.7.3"
  },
  "peerDependencies": {
    "solid-js": "^1.2"
  },
  "private": false,
  "description": "SFC for Solid",
  "repository": {
    "url": "https://github.com/lxsmnsyc/solid-sfc.git",
    "type": "git"
  },
  "homepage": "https://github.com/lxsmnsyc/solid-sfc/tree/main/packages/solid-sfc",
  "bugs": {
    "url": "https://github.com/lxsmnsyc/solid-sfc/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexis Munsayac",
  "scripts": {
    "prepublishOnly": "pridepack clean && pridepack build",
    "build": "pridepack build",
    "type-check": "pridepack check",
    "lint": "pridepack lint",
    "test": "pridepack test --passWithNoTests",
    "clean": "pridepack clean",
    "watch": "pridepack watch",
    "start": "pridepack start",
    "dev": "pridepack dev"
  }
}
